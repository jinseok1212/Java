package quiz01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		String a = scan.nextLine().trim();
		if(a.length() > 0) {
			String[] b = a.split(" ");
			System.out.println(b.length);
			
		} else {
			System.out.println(0);
		}
	}
}

