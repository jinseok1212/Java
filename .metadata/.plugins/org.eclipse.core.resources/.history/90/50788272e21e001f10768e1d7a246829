package quiz01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		int b = scan.nextInt();
		int aA = a;//원조 a
		int bB = b;//원조 b
		int min = 0;
		//최대공약수 공식
		while(b != 0) {
			int max = b;
			b = a % b;
			a = max;
		}
		//최소공배수 공식
		min = aA * bB / a;
		System.out.println(a);
		System.out.println(min);
	}
}
