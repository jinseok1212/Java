package quiz01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		String num = scan.next();
		int sum = 0;
		for(int i = 0; i < a; i++) {
			sum += num.charAt(i) - '0';
			
		}
		System.out.println(sum);
		
	}
}

