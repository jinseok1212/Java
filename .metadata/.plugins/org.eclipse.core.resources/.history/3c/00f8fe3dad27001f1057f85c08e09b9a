package quiz01;

import java.util.Scanner;

public class Main {

	public String[] solution(String[] strArr) {
        String[] answer = {};
        String[] ans = new String[strArr.length];
        for(int i = 0; i < strArr.length; i++) {
            if(strArr[i].indexOf("ad") == -1 ) {
                ans[i] = strArr[i];
            }
        }
        return ans;
    }
}
