package quiz17;

public class UserVO <T>{
	//회원정보를 담기 위한 클래스 -> 은닉, getter, setter
	
	private T name;
	private T age;
	
	//생성자
	public UserVO() {
	}
	
	public UserVO(T name, T age) {
		super();
		this.name = name;
		this.age = age;
	}

	public String getName() {
		return name;
	}
	public void setName(T name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(T age) {
		this.age = age;
	}
	
	//alt + shift + s
}
