package quiz01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		int c = scan.nextInt();
		
		int zero = 0;
		int one = 0;
		int two = 0;
		int three = 0;
		int four = 0;
		int five = 0;
		int six = 0;
		int seven = 0;
		int eight = 0;
		int nine = 0;
		int result = a * b * c;
		int re = a * b * c;
		for(int i = 1; i < result; i *= 10) {
			int w = re % i;
			int q = re / i;
//			for(int j = 0; j < 10; j++) {
//				
//			}
			if(w == 0) {
				zero++;
			} else if(w == 1) {
				one++;
			} else if(w == 2) {
				two++;
			} else if(w == 3) {
				three++;
			} else if(w == 4) {
				four++;
			} else if(w == 5) {
				five++;
			} else if(w == 6) {
				six++;
			} else if(w == 7) {
				seven++;
			} else if(w == 8) {
				eight++;
			} else if(w == 9) {
				nine++;
			}
		}
	
		
	}
}

