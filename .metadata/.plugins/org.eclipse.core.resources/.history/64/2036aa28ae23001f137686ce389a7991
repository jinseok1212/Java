package quiz01;
import java.util.*;
class Solution {
    public String solution(String myString) {
        String answer = "";
        for(int i = 0; i < myString.length(); i++) {
            if(myString.charAt(i) < 'l') {
                myString.replace(myString.charAt(i), 'l');
            }
        }
        return answer;
    }
}