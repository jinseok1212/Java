package day14.api.collection.queue;

import java.util.PriorityQueue;
import java.util.Queue;

public class PriorityQueueEx {
	
	public static void main(String[] args) {
		
		//우선순위 큐 => 자동 정렬기능
//		PriorityQueue<Integer> queue = new PriorityQueue<>();
		Queue<Integer> queue = new PriorityQueue<>();
		
		//추가
		queue.offer(5);
		queue.offer(2);
		queue.offer(3);
		queue.offer(10);
		queue.offer(6);
		
		//[2,3,5,6,10]
		System.out.println(queue.toString());
		//삭제(꺼냄)
		//보이는 거랑 다름에 주의
		System.out.println(queue.poll());
		System.out.println(queue.poll());
		System.out.println(queue.poll());
		System.out.println(queue.poll());
		System.out.println(queue.poll());		
		System.out.println(queue.poll());		
		
		//queue가 비웠는지 확인
		if(queue.isEmpty() == false) {//queue가 비워져있으면 true, 아니면 false
			System.out.println("큐가 비워있지 않음");
		}
		System.out.println("-------------------------------------");
		//UserVO를 담을 수 있는 Queue
		Queue<UserVO> que = new PriorityQueue<>();
		que.offer(new UserVO("홍길동", 20));
		que.offer(new UserVO("이순신", 30));
		que.offer(new UserVO("홍길자", 40));
	}

}
